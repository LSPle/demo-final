{
  "name": "database-optimization-platform",
  "version": "1.0.0",
  "description": "数据库优化平台前端",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "backend": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Set-Location ..\\backend; $env:DB_TYPE='mysql'; $env:MYSQL_HOST='127.0.0.1'; $env:MYSQL_PORT='3306'; $env:MYSQL_USER='root'; $env:MYSQL_PASSWORD='123456'; $env:MYSQL_DB='user'; if (Test-Path .\\.venv\\Scripts\\python.exe) { .\\.venv\\Scripts\\python.exe .\\run.py } elseif (Get-Command py -ErrorAction SilentlyContinue) { py -3 .\\run.py } else { python .\\run.py }\"",
    "frontend": "set PORT=3000&& react-scripts start",
    "dev": "npx concurrently -n \"backend,frontend\" -c \"blue,green\" \"npm:backend\" \"npm:frontend\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "socket.io-client": "^4.7.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@cloudbase/framework-plugin-container": "^1.9.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "concurrently": "^9.2.1",
    "http-proxy-middleware": "^2.0.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
